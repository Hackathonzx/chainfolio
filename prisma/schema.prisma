generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id            String   @id @default(cuid())
  githubId      String?  @unique
  walletAddress String?  @unique
  username      String?
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt
}
